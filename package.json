{
	"name": "backend",
	"version": "0.1.0",
	"bin": {
		"backend": "bin/backend.js"
	},
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"test": "jest",
		"cdk": "cdk",
		"list:output": "aws cloudformation describe-stacks --stack-name travel-viewer-app-stack-develop --query \"Stacks[].Outputs[0].OutputValue\"",
		"deploy:all": "npx aws-cdk deploy --outputs-file ./output.json",
		"whitelist:cloudinary": "touch focusotter-tuts && aws s3 cp focusotter-tuts s3://focus-otter-saas-images/.wellknown/cloudinary/ && rm focusotter-tuts"
	},
	"devDependencies": {
		"@aws-appsync/utils": "^1.1.0",
		"@types/git-branch": "^2.0.2",
		"@types/jest": "^29.4.0",
		"@types/node": "18.14.2",
		"aws-cdk": "2.67.0",
		"esbuild": "^0.17.11",
		"jest": "^29.4.3",
		"ts-jest": "^29.0.5",
		"ts-node": "^10.9.1",
		"typescript": "~4.9.5"
	},
	"dependencies": {
		"@aws-cdk/aws-cognito-identitypool-alpha": "^2.67.0-alpha.0",
		"aws-cdk-lib": "2.67.0",
		"constructs": "^10.0.0",
		"git-branch": "^2.0.1",
		"source-map-support": "^0.5.21"
	}
}
